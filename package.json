{
	"main": "node_modules/expo/AppEntry.js",
	"scripts": {
		"start": "expo start --tunnel",
		"android": "expo start --android",
		"prepare": "husky install",
		"ios": "expo start --ios",
		"web": "expo start --web",
		"eject": "expo eject",
		"test": "jest --detectOpenHandles --runInBand --forceExit",
		"lint": "eslint --config .eslintrc.js --fix . && prettier --config prettier.config.js --write \"**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml,scss}\"",
		"lint-check": "eslint --config .eslintrc.js . && prettier --config prettier.config.js --check \"**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml,scss}\"",
		"lint-staged": "pretty-quick --staged && lint-staged"
	},
	"dependencies": {
		"axios": "^0.21.4",
		"expo": "~42.0.1",
		"expo-cli": "^4.12.12",
		"expo-location": "~12.1.3",
		"expo-status-bar": "~1.0.4",
		"global": "^4.4.0",
		"react": "16.13.1",
		"react-dom": "16.13.1",
		"react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
		"react-native-maps": "0.28.0",
		"react-native-select-multiple": "^2.1.0",
		"react-native-vector-icons": "^8.1.0",
		"react-native-web": "~0.13.12"
	},
	"devDependencies": {
		"@babel/core": "^7.9.0",
		"@googlemaps/google-maps-services-js": "3.3.1",
		"@testing-library/react-hooks": "^7.0.2",
		"@testing-library/react-native": "^7.2.0",
		"@types/jest": "^27.0.2",
		"@types/react": "~16.9.35",
		"@types/react-native": "^0.65.0",
		"@types/react-native-vector-icons": "^6.4.9",
		"@types/react-test-renderer": "^17.0.1",
		"@typescript-eslint/eslint-plugin": "^4.31.0",
		"@typescript-eslint/parser": "^4.31.0",
		"babel-jest": "^27.2.2",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.25.1",
		"husky": "^7.0.4",
		"jest-expo": "^42.1.0",
		"lint-staged": "^11.1.2",
		"prettier": "^2.3.2",
		"pretty-quick": "^3.1.1",
		"react-test-renderer": "^17.0.2",
		"typescript": "~4.0.0"
	},
	"jest": {
		"projects": [
			{
				"preset": "jest-expo/ios"
			},
			{
				"preset": "jest-expo/android"
			}
		],
		"transformIgnorePatterns": [
			"node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
		]
	},
	"lint-staged": {
		"*.{ts,js}": "eslint --cache --fix"
	},
	"private": true
}
