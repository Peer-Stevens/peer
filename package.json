{
	"main": "node_modules/expo/AppEntry.js",
	"scripts": {
		"start": "expo start --tunnel",
		"android": "expo start --android",
		"prepare": "husky install",
		"ios": "expo start --ios",
		"web": "expo start --web",
		"eject": "expo eject",
		"test": "jest --watchAll",
		"build": "eas build --platform android --profile preview",
		"lint": "eslint --config .eslintrc.js --fix . && prettier --config prettier.config.js --write \"**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml,scss}\"",
		"lint-check": "eslint --config .eslintrc.js . && prettier --config prettier.config.js --check \"**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml,scss}\"",
		"lint-staged": "pretty-quick --staged && lint-staged"
	},
	"dependencies": {
		"@react-native-async-storage/async-storage": "1.15.17",
		"@unimodules/core": "7.2.0",
		"@unimodules/react-native-adapter": "6.5.0",
		"axios": "0.25.0",
		"crypto-js": "4.1.1",
		"email-validator": "2.0.4",
		"expo": "44.0.6",
		"expo-app-loading": "1.3.0",
		"expo-font": "10.0.5",
		"expo-haptics": "11.1.1",
		"expo-location": "14.0.2",
		"global": "4.4.0",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-native": "0.67.2",
		"react-native-maps": "0.30.1",
		"react-native-select-multiple": "2.1.0",
		"react-native-vector-icons": "9.0.0",
		"react-native-web": "0.17.5"
	},
	"devDependencies": {
		"@babel/core": "7.17.0",
		"@googlemaps/google-maps-services-js": "3.3.8",
		"@testing-library/react-hooks": "7.0.2",
		"@testing-library/react-native": "9.0.0",
		"@types/crypto-js": "4.1.0",
		"@types/jest": "27.4.0",
		"@types/react": "17.0.39",
		"@types/react-native": "0.66.15",
		"@types/react-native-vector-icons": "6.4.10",
		"@types/react-test-renderer": "17.0.1",
		"@typescript-eslint/eslint-plugin": "5.10.2",
		"@typescript-eslint/parser": "5.10.2",
		"babel-jest": "27.4.6",
		"eas-cli": "0.46.0",
		"eslint": "8.8.0",
		"eslint-config-prettier": "8.3.0",
		"eslint-plugin-prettier": "4.0.0",
		"eslint-plugin-react": "7.28.0",
		"expo-cli": "5.0.3",
		"husky": "7.0.4",
		"jest-expo": "44.0.1",
		"lint-staged": "12.3.3",
		"prettier": "2.5.1",
		"pretty-quick": "3.1.3",
		"react-test-renderer": "17.0.2",
		"typescript": "4.5.5"
	},
	"jest": {
		"projects": [
			{
				"preset": "jest-expo/ios"
			},
			{
				"preset": "jest-expo/android"
			}
		],
		"transformIgnorePatterns": [
			"node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
		]
	},
	"lint-staged": {
		"*.{ts,js}": "eslint --cache --fix"
	},
	"private": true
}
