{
	"main": "node_modules/expo/AppEntry.js",
	"scripts": {
		"start": "expo start --tunnel",
		"android": "expo start --android",
		"prepare": "husky install",
		"ios": "expo start --ios",
		"web": "expo start --web",
		"eject": "expo eject",
		"test": "jest --watchAll",
		"build": "eas build --platform android --profile preview",
		"lint": "eslint --config .eslintrc.js --fix . && prettier --config prettier.config.js --write \"**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml,scss}\"",
		"lint-check": "eslint --config .eslintrc.js . && prettier --config prettier.config.js --check \"**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml,scss}\"",
		"lint-staged": "pretty-quick --staged && lint-staged"
	},
	"dependencies": {
		"@react-native-async-storage/async-storage": "~1.15.0",
		"@unimodules/core": "7.2.0",
		"@unimodules/react-native-adapter": "6.5.0",
		"axios": "0.26.0",
		"crypto-js": "4.1.1",
		"email-validator": "2.0.4",
		"expo": "43.0.3",
		"expo-app-loading": "~1.2.1",
		"expo-font": "10.0.5",
		"expo-haptics": "~11.0.3",
		"expo-linking": "~2.4.2",
		"expo-location": "~13.0.4",
		"global": "4.4.0",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-native": "0.64.3",
		"react-native-maps": "0.28.1",
		"react-native-select-multiple": "2.1.0",
		"react-native-vector-icons": "9.1.0",
		"react-native-web": "0.17.6"
	},
	"devDependencies": {
		"@babel/core": "7.17.5",
		"@googlemaps/google-maps-services-js": "3.3.10",
		"@testing-library/react-hooks": "7.0.2",
		"@testing-library/react-native": "9.0.0",
		"@types/crypto-js": "4.1.1",
		"@types/jest": "27.4.0",
		"@types/react": "17.0.39",
		"@types/react-native": "0.66.16",
		"@types/react-native-vector-icons": "6.4.10",
		"@types/react-test-renderer": "17.0.1",
		"@typescript-eslint/eslint-plugin": "5.12.1",
		"@typescript-eslint/parser": "5.12.1",
		"babel-jest": "27.5.1",
		"eas-cli": "0.47.0",
		"eslint": "8.9.0",
		"eslint-config-prettier": "8.4.0",
		"eslint-plugin-prettier": "4.0.0",
		"eslint-plugin-react": "7.28.0",
		"expo-cli": "^5.2.0",
		"husky": "7.0.4",
		"jest-expo": "43.0.1",
		"lint-staged": "12.3.4",
		"peer-types": "^1.0.2",
		"prettier": "2.5.1",
		"pretty-quick": "3.1.3",
		"react-test-renderer": "17.0.2",
		"typescript": "4.5.5"
	},
	"jest": {
		"projects": [
			{
				"preset": "jest-expo/ios"
			},
			{
				"preset": "jest-expo/android"
			}
		],
		"transformIgnorePatterns": [
			"node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
		]
	},
	"lint-staged": {
		"*.{ts,js}": "eslint --cache --fix"
	},
	"private": true
}
